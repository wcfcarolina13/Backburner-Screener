{
  "name": "backburner-screener",
  "version": "1.0.0",
  "description": "Real-time MEXC screener for The Chart Guys Backburner trading strategy",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc && cp -r src/views dist/",
    "start": "node dist/web-server.js",
    "web": "node dist/web-server.js",
    "dev": "tsx src/index.ts",
    "dev:web": "tsx watch src/web-server.ts",
    "watch": "tsx watch src/index.ts",
    "check": "tsx src/check-symbol.ts",
    "analyze": "tsx src/analyze.ts",
    "summary": "tsx scripts/daily-summary.ts",
    "summary:today": "tsx scripts/daily-summary.ts",
    "backtest": "tsx src/backtest-cli.ts",
    "backtest-combined": "tsx src/backtest-combined.ts",
    "backtest-combined-candles": "tsx src/backtest-combined-candles.ts",
    "debug-signals": "tsx src/debug-signals.ts",
    "backtest-combined-db": "tsx src/backtest-combined-db.ts",
    "prefetch": "tsx src/prefetch-candles.ts",
    "generate-signals": "tsx src/generate-signals.ts",
    "focus": "node dist/focus-mode-dashboard.js",
    "focus:dev": "tsx src/focus-mode-dashboard.ts",
    "backtest:leveraged": "node dist/backtest-leveraged-regime.js",
    "backtest:spot": "node dist/backtest-spot-regime-realistic.js",
    "spot-backtest": "node dist/spot-backtest-cli.js"
  },
  "keywords": [
    "trading",
    "crypto",
    "mexc",
    "screener",
    "backburner",
    "rsi"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@libsql/client": "^0.17.0",
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.5",
    "ejs": "^4.0.1",
    "express": "^5.2.1",
    "node-notifier": "^10.0.1",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/node": "^22.10.2",
    "@types/node-notifier": "^8.0.5",
    "@types/ws": "^8.5.13",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
